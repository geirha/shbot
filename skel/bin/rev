#!/bin/gawk4 -f

function rev(s,   c,i,n,r) {
    n = length(s);
    for (i=n;i>=1;--i) {
        c = substr(s, i, 1);
        if (c in mapping)
            c = mapping[c];
        r = r c;
    }
    return r;
}

BEGIN {
    RS="[ \t\n]+";
    mapping["("]=")";
    mapping["["]="]";
    mapping["{"]="}";
    mapping["<"]=">";
    mapping[")"]="(";
    mapping["]"]="[";
    mapping["}"]="{";
    mapping[">"]="<";
}
{ printf("%s%s", rev($0), RT); }
